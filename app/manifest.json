{
  "manifest_version": 2,
  "name": "Twitter Lists Redux",
  "version": "3.0.4",

  "description": "Quick and easy access to Twitter Lists.",
  "author": "tomitm",

  "icons": { "128": "icon128.png" },

  "minimum_chrome_version": "49",
  "permissions": [
    "https://twitter.com/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "contentscript.js"
      ],
      "css": [
        "main.css"
      ]
    }
  ]
}
